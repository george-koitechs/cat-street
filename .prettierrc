{
  "printWidth": 120,
  "jsxSingleQuote": true,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",

  "importOrder": [
    "^(gatsby$)",
    "^(react$)",
    "<THIRD_PARTY_MODULES>",
    "^[./]",
    "^(.*).scss$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
